<?xml version='1.0' encoding='utf-8'?>
<plugin id="TargetAdder" version="0.1"
	xmlns="http://apache.org/cordova/ns/plugins/1.0"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<name>TargetAdder</name>
	<!--<js-module name="TargetAdder" src="www/TargetAdder.js">
		<clobbers target="cordova.plugins.TargetAdder" />
	</js-module> -->
	<platform name="ios">
		<preference name="TARGET_NAME" />
		<preference name="BUNDLE_ID" />
		<!-- Hooks for Swift Support
		<hook type="before_plugin_install" src="hooks/install_prerequisites.js"/>
		<hook type="after_plugin_add" src="hooks/add_swift_support.js" />
		-->
		<hook type="before_plugin_install" src="hooks/install_prerequisites.js"/>
		<hook type="after_plugin_install" src="hooks/create_target.js"/>

		<config-file parent="/*" target="config.xml">
			<feature name="outsystems-secondtargetadder-plugin">
				<param name="ios-package" value="outsystems-secondtargetadder-plugin" />
			</feature>
		</config-file>
		<!-- <source-file src="src/ios/outsystems-secondtargetadder-plugin.m" /> -->

	</platform>
</plugin>